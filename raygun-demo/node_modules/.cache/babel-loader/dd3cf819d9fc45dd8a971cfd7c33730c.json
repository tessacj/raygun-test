-for=\"tvshows in tvshowChunks\" :key=\"tvshows\">\r\n      <div class=\"place\" v-for=\"tvshow in tvshows\" :key=\"tvshow\">\r\n        <h2>{{ tvshow.original_title }}</h2>\r\n        <img v-if=\"tvshow.poster_path\" :src=\"'https://image.tmdb.org/t/p/w500'+tvshow.poster_path\" width=\"235\" height=\"300\"/>\r\n        <slot></slot>\r\n        <p>{{ tvshow.overview }}</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport _ from 'lodash'\r\nimport { getPageData } from '../services/PageService'\r\n\r\nexport default {\r\n  name: 'TVShowComponent',\r\n  data () {\r\n    return {\r\n      data: \"page3\",\r\n      results: [],\r\n      trending: []\r\n    }\r\n  },\r\n  mounted() {\r\n    getPageData(this.data).then(response => {\r\n      this.trending = response.results\r\n      console.log(this.trending)\r\n    })\r\n\r\n  },\r\n  computed: {\r\n    tvshowChunks(){\r\n      return _.chunk(Object.values(this.trending), 5)\r\n    }\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\nh3 {\r\n  margin: 40px 0 0;\r\n}\r\nul {\r\n  list-style-type: none;\r\n  padding: 0;\r\n}\r\nli {\r\n  display: inline-block;\r\n  margin: 0 10px;\r\n}\r\na {\r\n  color: #42b983;\r\n}\r\n\r\nh1 {\r\n  text-align: center;\r\n}\r\n\r\n.tvshow-contain {\r\n  display: flex;\r\n  justify-content: center;\r\n  margin-top: 20px;\r\n}\r\n\r\n.place {\r\n  display: flex;\r\n  flex-direction: column;\r\n  width: 280px;\r\n  height: 600px;\r\n  justify-content: center;\r\n  background: white;\r\n  border: 1px solid #ddd;\r\n  padding: 20px 20px;\r\n  margin: 10px;\r\n}\r\n.place h2 {\r\n    margin: 0;\r\n    text-align: center;\r\n  }\r\n.place img {\r\n    margin: 10px;\r\n    align-self: center;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/components"}]}