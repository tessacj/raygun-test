endingComponent',\n  data () {\n    return {\n      data: \"page1\",\n      results: [],\n      trending: []\n    }\n  },\n  mounted() {\n    getPageData(this.data).then(response => {\n      this.trending = response.results\n      console.log(this.trending)\n    })\n\n  },\n  computed: {\n    trendingChunks(){\n      return _.chunk(Object.values(this.trending), 5)\n    }\n\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n\nh1 {\n  text-align: center;\n}\n\n.trending-contain {\n  display: flex;\n  justify-content: center;\n  margin-top: 20px;\n}\n\n.place {\n  display: flex;\n  flex-direction: column;\n  width: 280px;\n  height: 600px;\n  justify-content: center;\n  background: white;\n  border: 1px solid #ddd;\n  padding: 20px 20px;\n  margin: 10px;\n}\n.place h2 {\n    margin: 0;\n    text-align: center;\n  }\n.place img {\n    margin: 10px;\n    align-self: center;\n  }\n</style>\n"]}]}